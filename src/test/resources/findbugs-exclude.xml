<FindBugsFilter>
    <!-- List of bug patterns that are usually generating false positives. -->
    <Match>
        <Bug pattern="SF_SWITCH_NO_DEFAULT,SF_SWITCH_FALLTHROUGH,SIC_INNER_SHOULD_BE_STATIC_ANON,BC_UNCONFIRMED_CAST,DMI_RANDOM_USED_ONLY_ONCE,CO_ABSTRACT_SELF,DM_EXIT,URF_UNREAD_PUBLIC_OR_PROTECTED_FIELD,UWF_UNWRITTEN_PUBLIC_OR_PROTECTED_FIELD,DLS_DEAD_LOCAL_STORE"/>
    </Match>
</FindBugsFilter>